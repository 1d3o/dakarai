<%
  row = cell(:elements, :row).new
  block = cell(:elements, :block).new(class: 'xs-12 sm-12 md-12')
%>

<!-- MAIN CONTAINER -->
<%=raw row.open %>

  <%=raw block.open %>

    <%=raw cell(:elements, :title).new(label: @data.name, size: 2) %>

  <%=raw block.close %>

<%=raw row.close %>
<!-- / MAIN CONTAINER -->